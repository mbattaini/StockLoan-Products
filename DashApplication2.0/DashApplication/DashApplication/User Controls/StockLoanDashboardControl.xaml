<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    	xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml" x:Class="DashApplication.StockLoanDashboardControl"
    mc:Ignorable="d" FontFamily="TitilliumText22L" Loaded="UserControl_Loaded_1">

    <Grid x:Name="LayoutRoot" Background="White" RenderTransformOrigin="0.5,0.5">
        <c1:C1ToolbarStrip Height="24" Margin="8,8,8,0" BorderBrush="#FFB2E0F4" Background="#FFB2E0F4">
            <sdk:Label Content="Business Date:" Height="18" VerticalAlignment="Center" HorizontalAlignment="Right" Width="83" Style="{StaticResource LabelStyle}"/>
            <c1:C1DateTimePicker x:Name="BizDatePicker" Width="134" EditMode="Date" HorizontalAlignment="Right" Margin="0" Padding="0" Style="{StaticResource C1BizDatePickerStyle}"/>
        </c1:C1ToolbarStrip>
        <c1:C1TabControl x:Name="CtrlTabControl" Margin="10,37,0,0"/>
    </Grid>
</UserControl>
